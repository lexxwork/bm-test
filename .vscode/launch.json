  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "worker test",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/src/workers/etherscan.ts",
      "preLaunchTask": "tsc: build - tsconfig.worker.json",
      "envFile": "${workspaceFolder}/.env.local",
      "outFiles": [
        "${workspaceFolder}/workers-build/**/*.js"
      ]
